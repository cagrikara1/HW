#include <stdio.h>
#include <math.h> 
void computeHomeValue(void);

int main(void) {
    computeHomeValue();
    return 0;
}

void computeHomeValue(void) {
    int popularity, size;
    long long homeValue;

    // Prompt user for input
    printf("Enter popularity: ");
    scanf("%d", &popularity);

    printf("Enter size: ");
    scanf("%d", &size);

    // Compute home value
    homeValue = (pow(popularity, 3) + pow(size, 2)) * 10000;

    // Display the computed home value
    printf("Home value is: %lld TL\n", homeValue);
}
